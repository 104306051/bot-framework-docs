{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md", 
          "**/**.yml", 
          "dotnet/**.md", 
          "node/**.md"
        ],
        "exclude": [
          "**/obj/**",
          "BotFramework-Docs/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "**/_deprecated/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "BotFramework-Docs/**",
          "~/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-BotFramework",
      "breadcrumb_path": "/botframework/breadcrumb/TOC.json",
      "author": "RobStand",
      "ms.prod": "botframework",
      "ms.date": "03/31/2017"
    },
    "fileMetadata": {
      "ms.date":
       {
         "**/*.md": "03/31/2017"
       },
       "manager":
       {
         "**/*.md": "rstand"
       }
    },
    "template": [],
    "dest": "BotFramework-Docs"
  }
}
